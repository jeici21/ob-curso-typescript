{
  "name": "ts-demo",
  "version": "1.0.0",
  "description": "Proyecto de ejemplo para el desarrollo con TS",
  "main": "index.js",
  "scripts": {
    "tsNode": "cd src && ts-node index.ts",
    "start": "nodemon",
    "linting": "eslint",
    "linting:fix": "eslint --fix",
    "document": "typedoc --out docs src",
    "transpilation": "tsc",
    "build:prod": "rimraf ./build && tsc",
    "start:prod": "npm run build:prod && node build/index.js"
  },
  "keywords": [
    "typescript",
    "ts"
  ],
  "author": "jeici21",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.21",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "cookies-utils": "^1.0.0"
  }
}